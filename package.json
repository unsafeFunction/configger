{
  "name": "mirimus-admin",
  "version": "7.6.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,scss,less}\"",
    "eslint": "eslint --fix --ext .js,jsx src",
    "stylelint": "stylelint \"src/**/*.{scss,less}\"",
    "build:qa": "env-cmd -f .env.qa npm run build",
    "build:production": "env-cmd -f .env.prod npm run build",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,scss,less}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint"
    ],
    "src/**/*.{scss,less}": [
      "stylelint"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.6",
    "@sentry/react": "^5.29.2",
    "antd": "4.16.13",
    "axios": "^0.19.2",
    "bootstrap-css-only": "^4.3.1",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.4.0",
    "cookie-storage": "^6.0.0",
    "customize-cra": "^0.2.13",
    "env-cmd": "^10.1.0",
    "history": "^4.9.0",
    "lodash.debounce": "^4.0.8",
    "lodash.differenceby": "^4.8.0",
    "lodash.filter": "^4.6.0",
    "lodash.find": "^4.6.0",
    "lodash.floor": "^4.0.4",
    "lodash.foreach": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.isempty": "^4.4.0",
    "lodash.map": "^4.6.0",
    "lodash.merge": "^4.6.2",
    "lodash.omit": "^4.5.0",
    "lodash.orderby": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.32",
    "nprogress": "^0.2.0",
    "qs": "^6.9.3",
    "react": "^17.0.2",
    "react-app-rewired": "^2.1.3",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-infinite-scroll-component": "^5.1.0",
    "react-input-mask": "^2.0.4",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-router-transition-switch": "1.1.1",
    "react-scripts": "3.0.1",
    "react-transition-group": "^4.2.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.0.3",
    "reselect": "^4.0.0",
    "store": "^2.0.12"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "babel-plugin-import": "^1.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^2.4.1",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^8.2.1",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^3.1.0",
    "prop-types": "^15.7.2",
    "redux-logger": "^3.0.6",
    "standard-version": "^9.1.0",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^18.3.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
